{
  "name": "marybot",
  "version": "0.1.0",
  "description": "Discord bot para comunidade de animes - MaryBot",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon --watch src --exec node src/index.js",
    "deploy": "node src/deploy-commands.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:reset": "prisma db push --force-reset",
    "db:seed": "node prisma/seed.js"
  },
  "engines": {
    "node": ">=16.9.0"
  },
  "dependencies": {
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2",
    "prisma": "^5.6.0",
    "@prisma/client": "^5.6.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "discord",
    "bot",
    "anime",
    "community",
    "nodejs",
    "prisma",
    "postgresql"
  ],
  "author": "LyeZinho",
  "license": "MIT"
}
